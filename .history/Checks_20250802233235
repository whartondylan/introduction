
  variant
}: CheckboxOrRadioGroupArgs) => {
  const parentArgs = {
    disabled,
    required
  };
  const labelArgs = {
    children: labelChildren,
    visuallyHidde
  };
  const validationArgs = {
    children: validationChildren,
    variant
  };
  return <RadioGroup {...parentArgs} name="defaultRadioGroup">
      {labelArgs.children && <RadioGroup.Label {...labelArgs} />}
      {captionChildren && <RadioGroup.Caption>{captionChildren}</RadioGroup.Caption>}
      <FormControl>
        <Radio value="one" />
        <FormControl.Label>Choice one</FormControl.Label>
      </FormControl>
      <" defaultChecked />
  .Label>Choice two</FormControl.Label>
      </FormControl>
      <FormControl>
        <Radio value="three" />
        <FormControl.Label>Choice three</FormControl.Label>
      </FormControl>
      {validationArgs.children && <RadioGroup.Validation {...validationArgs} />}
    </RadioGroup>;
}
